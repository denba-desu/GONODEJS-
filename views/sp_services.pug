doctype html
html
	head
		title iTutor
		meta(charset='utf-8')
		meta(name='viewport', content='width=device-width, initial-scale=1')
		link(rel='stylesheet', href='assets/css/main.css')
		link(rel='stylesheet', href='assets/css/ie8.css')

	body

		section(id='header')
			header
				img(src='images/logo.png' alt="")
			nav(id='nav')
				ul
					li
						a(href='/current_services') Current Services
						|
						|
					li
						a(href='service_requests') Service Requests
						|
						|
					li
						a(href='history') History
						|
						|
					li
						a(href='profile' class="active") Profile
						|
						|
					li
						a(href='logout') Log-out

			footer
				ul(class='icons')
					li
						a(href='#' class='icon fa-twitter')
						span(class='label') Twitter
					li
						a(href='#' class='icon fa-facebook')
						span(class='label') Facebook
					li
						a(href='#' class='icon fa-instagram')
						span(class='label') Instagram
					li
						a(href='#' class='icon fa-github')
						span(class='label') Github
					li
						a(href='#' class='icon fa-envelope')
						span(class='label') Email

		div(id='wrapper')
			div(id='main')
				section(id='one')
					div(class='container')
						header(class='major')
							h2= 'Your Services'


						table(id='myTable')
							tr
									th= 'Service ID'
									th= 'Service Name'
									th= 'Rate'

							for personalServices in personalServicesList
								tr
									td= personalServices.service_id
									td= personalServices.service_name
									td= personalServices.service_rate


script(src='assets/js/jquery.min.js')
script(src="assets/js/jquery.scrollzer.min.js")
script(src="assets/js/jquery.scrolly.min.js")
script(src="assets/js/skel.min.js")
script(src="assets/js/util.js")
script(src="assets/js/main.js")